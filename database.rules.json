{
  "rules": {
    "answers": {
      ".read": "auth != null",
      ".write": "auth != null",
    },
    "questions": {
      ".read": "auth != null",
    },
    "users": {
      "$stdNum": {
        ".read": "auth != null && auth.token.email.contains($stdNum)",
        ".write": "auth != null && auth.token.email.contains($stdNum)",
      }
    }
  }
}